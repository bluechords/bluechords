type User {
    id: ID! @id
    email: String! @unique
    password: String!
    songs: [Song!]! @relation(link: INLINE)
    createdAt: DateTime! @createdAt
}

type Song {
    id: ID! @id
    name: String!
    content: String
    createdBy: User!
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}
